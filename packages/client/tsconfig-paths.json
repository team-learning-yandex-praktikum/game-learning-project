{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@types": ["src/types"],
      "@types/*": ["src/types/*"],
      "@routes": ["src/routes"],
      "@routes/*": ["src/routes/*"],
      "@store": ["src/store"],
      "@store/*": ["src/store/*"],
      "@styles": ["src/styles"],
      "@styles/*": ["src/styles/*"],
      "@api": ["src/api"],
      "@api/*": ["src/api/*"]
    },
  }
}
